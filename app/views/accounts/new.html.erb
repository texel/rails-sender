<h1>New account</h1>

<% form_for(@account, :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :api_email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :api_password %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<p><%= link_to 'Back', accounts_path %></p>
